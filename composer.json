{
  "require": {
    "ext-imagick": "*",
    "calcinai/php-imagick": "dev-master",
    "craftcms/aws-s3": "1.3.0",
    "craftcms/cms": "^3.6.4",
    "craftcms/redactor": "2.8.8",
    "sebastianlenz/linkfield": "1.0.25",
    "spicyweb/craft-embedded-assets": "2.9.1",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.0",
      "ext-imagick": "3.6.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
